# API Key for frontend to authenticate with System Logic Service
FRONTEND_API_KEY=your-frontend-api-key-here

# API Key for System Logic Service to authenticate with LLM Service
LLM_SERVICE_API_KEY=your-llm-service-api-key-here

# LLM Service Configuration
# Use docker compose service name when containers are connected over docker network
LLM_SERVICE_URL=http://localhost:8001

# Server Configuration
HOST=0.0.0.0
PORT=8000
